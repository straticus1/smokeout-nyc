# Google Maps API Key
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Mapbox Access Token
REACT_APP_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# API Base URL
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Application Configuration
REACT_APP_APP_NAME=SmokeoutNYC
REACT_APP_VERSION=2.0.0

# OAuth2 Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_MICROSOFT_CLIENT_ID=your-microsoft-client-id
REACT_APP_TWITTER_API_KEY=your-twitter-api-key

# PayPal Configuration
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id

# Feature Flags
REACT_APP_ENABLE_GAMING=true
REACT_APP_ENABLE_AI_RISK=true
REACT_APP_ENABLE_DONATIONS=true
REACT_APP_ENABLE_NFT=true
REACT_APP_ENABLE_AR=false
REACT_APP_ENABLE_VOICE=false

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
